<Window xmlns:Views="clr-namespace:ClinicalGoal.Views"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ViewModels="clr-namespace:ClinicalGoal.ViewModels"  x:Class="ClinicalGoal.Views.MainWindow"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        mc:Ignorable="d"
        Title="Clinical Goal" d:DesignHeight="450" d:DesignWidth="800" d:DataContext="{d:DesignInstance {x:Type ViewModels:MainWindowViewModel}}"
        SizeToContent="WidthAndHeight">
    <Grid>
        <Views:ClinicalGoalView DataContext="{Binding ClinicalGoalViewModel}"/>
    </Grid>
</Window>
